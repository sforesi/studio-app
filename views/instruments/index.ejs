<%- include("../partials/html-head") %>
<%- include("../partials/nav") %>
<main>
  <section>
    <h1>INSTRUMENT & EQUIPMENT LIST</h1>
    <ul>
      <% instruments.forEach(instrument => { %>
        <li>
          <a href="/instruments/<%= instrument._id %>"><%= instrument.name %></a>
          <%= instrument.reserved ? "⛔️" : "✅" %>
        </li>
      <% }) %> 
    </ul>
  </section>
  <section>
    <a href="/instruments/new"><i class="fas fa-drum"></i><i class="fas fa-plus-circle"></i></a>
  </section>
</main>

<%- include("../partials/footer") %> 